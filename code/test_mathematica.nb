          RowBox[{
           RowBox[{"BoseEinstein", "[", 
            RowBox[{
             RowBox[{"omegaList", "[", 
              RowBox[{"[", "k", "]"}], "]"}], ",", "T"}], "]"}], "*", 
           RowBox[{"Exp", "[", 
            RowBox[{
             RowBox[{"-", "I"}], "*", 
             RowBox[{"omegaList", "[", 
              RowBox[{"[", "k", "]"}], "]"}], "*", "tau"}], "]"}]}], "+", 
          RowBox[{
           RowBox[{"(", 
