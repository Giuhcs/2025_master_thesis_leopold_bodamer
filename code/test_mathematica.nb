             RowBox[{"BoseEinstein", "[", 
              RowBox[{
               RowBox[{"omegaList", "[", 
                RowBox[{"[", "k", "]"}], "]"}], ",", "T"}], "]"}], "+", "1"}],
             ")"}], "*", 
           RowBox[{"Exp", "[", 
            RowBox[{"I", "*", 
             RowBox[{"omegaList", "[", 
              RowBox[{"[", "k", "]"}], "]"}], "*", "tau"}], "]"}]}]}], 
         ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", 
         RowBox[{"Length", "[", "omegaList", "]"}]}], "}"}]}], "]"}]}], ";"}],
    "\[IndentingNewLine]", "\n", 
