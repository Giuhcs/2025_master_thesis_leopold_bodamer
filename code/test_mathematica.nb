 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Define", " ", "the", " ", "correlation", " ", "function"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"MyCorrelationFunction", "[", 
      RowBox[{"tau_", ",", "gList_", ",", "omegaList_", ",", "T_"}], "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
